        "lint:stylelint-fix": "stylelint --config .stylelint.config.cjs  src/**/*.{vue,scss,css,sass,less} --fix",
        "lint-all": "pnpm run lint:prettier && pnpm run lint:es-fix && pnpm run lint:stylelint-fix",
        "prepare": "husky install",
        "commitlint": "commitlint --config commitlint.config.cjs -e -V"
    },
    "dependencies": {
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@ainiteam/quick-vue3-ui": "^1.2.1-alpha.2",
