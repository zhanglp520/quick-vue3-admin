        "@commitlint/config-conventional": "^17.7.0",
        "@element-plus/icons-vue": "^2.1.0",
        "@grapecity/spread-sheets": "^16.1.4",
        "@grapecity/spread-sheets-resources-zh": "^16.1.4",
        "@grapecity/spread-sheets-vue": "^16.1.4",
        "@types/node": "^20.5.0",
