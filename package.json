{
    "name": "quick-vue3-admin",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "build:tsc": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint:prettier": "prettier --write \"./**/*.{html,vue,js,ts,json,md}\" ",
